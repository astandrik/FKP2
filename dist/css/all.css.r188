.accordionTree * {
  box-sizing: border-box;
  font-family: 'Open Sans', sans-serif;
  font-weight: 300;
}
.accordionTree ul.accordion {
  border-left: 1px solid #ccc;
  border-right: 1px solid #ccc;
}
.accordionTree .defaultBorder {
  border: 1px solid black;
}
.accordionTree ul.accordion > li:first-child {
  margin-top: 0;
}
.accordionTree a {
  text-decoration: none;
  color: inherit;
  min-height: 80px;
}
