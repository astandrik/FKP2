  font-size: 1.1em;
  margin: 1em 0;
}
.accordionTree .description {
  margin: 1em auto 2.25em;
}
.accordionTree body {
  width: 40%;
  min-width: 300px;
  max-width: 400px;
  margin: 1.5em auto;
  color: #333;
}
.accordionTree h1 {
  font-family: 'Pacifico', cursive;
  font-weight: 400;
  font-size: 2.5em;
}
.accordionTree ul {
  list-style: none;
  padding: 0;
}
.accordionTree ul > li {
  border-top: #eee 1px solid;
  border-bottom: #eee 1px solid;
}
.accordionTree ul > li:first-child {
  border-top: 1px solid #666;
}
.accordionTree ul > li:last-child {
  border-bottom: 1px solid  #666;
}
.accordionTree ul .inner {
  overflow: hidden;
  display: none;
}
.accordionTree ul .inner span {
  padding-left: 1.2em;
}
.accordionTree ul .inner .inner span {
  padding-left: 2.4em;
}
.accordionTree ul .inner .inner .inner span {
  padding-left: 3.6em;
}
.accordionTree ul .inner .inner .inner .inner span {
  padding-left: 4.8em;
}
.accordionTree ul .inner .inner .inner .inner .inner span {
  padding-left: 6em;
}
.accordionTree ul .inner .inner .inner .inner .inner .inner span {
  padding-left: 7.2em;
}
.accordionTree ul .inner.show {
  /*display: block;*/
}
.accordionTree ul li a.toggle {
  width: 100%;
  background: #FFFFFF;
  color: black;
  padding: .75em;
  text-decoration: none;
  -webkit-transition: background .3s ease;
  transition: background .3s ease;
}
.accordionTree ul li a.toggle:hover {
  background: #D6D6D6;
}
.accordionTree ul li a.selected[type="2"] {
  background: #76A5AF;
  color: white;
}
.accordionTree ul li a.selected[type="1"] {
  background: #B4A7D5;
  color: white;
}
.accordionTree ul li a.selected[type="0"] {
  background: #D6D6D6;
}
.accordionTree ul li a.selected[type="3"] {
  background: #D6D6D6;
}
.accordionTree ul li span {
  max-width: 80%;
  font-size: 16px;
}
.accordionTree ul li a.fullWidth span {
  max-width: 90%!important;
}
.accordionTree ng-md-icon > svg {
  -webkit-transition: all 0.1s ease;
  transition: all 0.1s ease;
}
.accordionTree ng-md-icon > svg.show {
  -webkit-transform: rotate(90deg);
          transform: rotate(90deg);
}

